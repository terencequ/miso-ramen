{
  "name": "miso-ramen",
  "author": "Terence Qu",
  "version": "1.0.0",
  "private": true,
  "main": "public/main.js",
  "homepage": "./",
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-beta.2",
    "@material-ui/icons": "^5.0.0-beta.1",
    "@oatmilk/oat-milk-backend-typescript-axios-sdk": "^1.0.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.19",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "electron-is-dev": "^2.0.0",
    "env-cmd": "^10.1.0",
    "find-process": "^1.4.5",
    "lodash.throttle": "^4.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.3.5",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "dev": "react-app-rewired start",
    "build": "react-app-rewired build",
    "start": "concurrently \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.8",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "electron": "^15.1.1",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.2.0",
    "react-app-rewired": "^2.1.8",
    "wait-on": "^6.0.0"
  },
  "build": {
    "appId": "com.terencequ.misoramen",
    "compression": "normal",
    "productName": "Miso Ramen",
    "directories": {
      "buildResources": "electron-build",
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "public/win-logo.ico"
    },
    "extends": null
  }
}
